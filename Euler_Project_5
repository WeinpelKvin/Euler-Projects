public class EulerProblem5 {
	public static void main(String[] args) {
		int n = 15;
		int num = 2;
		for (int i = 1; i <= n; i++) { 	// Goes through the numbers from 1 - 15
			if (num % i == 0) {			// See if the number i is evenly divisible 
			} else {					// If its not then add 2 to the number and start over at 1
				i = 0;
				num += 2;
			}
		}		
		System.out.println(num);		// Once all numbers are evenly divisible print the largest number

	}
}
